        "def pascal_triangle(n):\n",
        "    row = [1]\n",
        "    rows = [row]\n",
        "    for i in range(1, n):\n",
        "        new_row = [1]\n",
        "        for j in range(1, i):\n",
        "            new_row.append(row[j-1] + row[j])\n",
        "        new_row.append(1)\n",
        "        rows.append(new_row)\n",
        "        row = new_row\n",
        "\n",
        "    for row in rows:\n",
        "        print(\" \".join([str(valeur) for valeur in row]))\n",
        "\n",
        "\n",
