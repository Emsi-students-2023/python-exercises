        "def decompress(liste):\n",
        "    if not liste:\n",
        "        return []\n",
        "    else:\n",
        "        valeur, loccurence = liste[0:2]\n",
        "        return [valeur] * loccurence + decompress(liste[2:])\n",
        "\n",
        "\n",
        "lst = [\"A\", 13, \"B\", 4, \"A\", 6, \"B\", 1]\n",
        "print(decompress(lst))\n"
